defaults
  mode http
  timeout client 15s
  timeout connect 10s
  timeout server 15s
  timeout http-request 10s

frontend johngit23-tech-frontend
    bind *:80
    default_backend johngit23-tech-backend

backend johngit23-tech-backend
    balance roundrobin
    server 330420-web-01 100.25.129.130:80 check
    server 330420-web-02 35.153.50.107:80 check
